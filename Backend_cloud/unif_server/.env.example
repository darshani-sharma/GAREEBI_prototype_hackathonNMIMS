# GAREEBI Server — Environment Variables
# Copy this file to .env and fill in your values

# ── Database ──
DB_NAME=gareebi_db
DB_USER=gareebi_user
DB_PASS=change_me_strong_password

# ── JWT ──
# IMPORTANT: Change to a cryptographically secure secret (min 256  bits / 32 chars)
JWT_SECRET=change_me_to_a_very_long_random_string_at_least_32_chars
JWT_EXPIRY_MS=86400000

# ── Redis ──
REDIS_HOST=cache
REDIS_PORT=6379
